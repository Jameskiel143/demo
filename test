<body>

<header>
    <h1>Welcome to My jQuery Website</h1>
</header>

<div id="content">
    <h2>Click the Button</h2>
    <button id="myButton">Click Me!</button>
    <div id="output"></div>
</div>

<footer id="footer">
    &copy; 2024 jQuery Website
</footer>

<script>
    $(document).ready(function(){
        $("#myButton").click(function(){
            $("#output").html("<p>You clicked the button!</p>");
        });
    });
</script>

<script>
    // Initialize Git repository
    $(document).ready(function(){
        // Initialize Git repository
        var gitInit = function() {
            $.ajax({
                type: "GET",
                url: "https://api.github.com/user",
                headers: {
                    Authorization: "Bearer YOUR_GITHUB_ACCESS_TOKEN"
                },
                success: function(response) {
                    console.log("GitHub Initialized");
                },
                error: function(xhr, status, error) {
                    console.error("Error initializing GitHub: " + error);
                }
            });
        };
        gitInit();
    });
</script>

</body>
</html>
